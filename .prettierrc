{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "sortImports": true,
  "importOrder": ["^react", "^@?\\w", "^\\.\\w", "^\\./"],
  "importDeclarationSortAscending": true
}
